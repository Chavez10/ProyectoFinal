
@{
    ViewBag.Title = "Usuarios";
    Layout = "~/Views/Shared/_Layout.cshtml";

}

<h2>Listado de los Usuarios</h2>
<hr />
<div class="row">
    <div class="col-lg-12">
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th>COD_USER</th>
                    <th>NOM_USER</th>
                    <th>MAIL_USER</th>
                    <th>COD_ROL</th>
                    <th>COD_ESTADO</th>
                    <th>Acciones</th>
                </tr>
            </thead>
            @foreach (var valores in Model)
            {
                <tbody>
                    <tr>
                        <td>@valores.COD_USER</td>
                        <td>@valores.NOM_USER</td>
                        <td>@valores.MAIL_USER</td>
                        <td>@valores.COD_ROL</td>
                        <td>@valores.COD_ESTADO</td>
                    </tr>
                </tbody>
                
            }
        </table>

    </div>

</div>

